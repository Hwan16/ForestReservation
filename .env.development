# 개발 환경 환경 변수 설정
# 실제 값으로 대체하거나 .env.development.local에 실제 값을 설정하세요.

# Supabase 설정
NEXT_PUBLIC_SUPABASE_URL=your-supabase-project-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# 데이터베이스 연결 정보 
DATABASE_URL=postgresql://postgres:password@localhost:5432/forest_dev
# 또는 Supabase를 사용하는 경우:
# DATABASE_URL=postgresql://postgres:password@db.your-project-id.supabase.co:5432/postgres

# SMS 서비스 설정 (개발 환경에서는 콘솔 출력만 가능)
ALIGO_KEY=your-aligo-key
ALIGO_USER_ID=your-aligo-user-id
SMS_SENDER=your-sender-phone-number
SMS_RECEIVER=your-sender-phone-number
SMS_TEST_MODE=true

# 관리자 계정 초기 설정
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin123

# 세션 보안 설정
SESSION_SECRET=dev-session-secret-key-change-this-in-production

# 배포 환경 설정
NODE_ENV=development

# 개발 서버 설정
PORT=3000
HOST=localhost
VITE_PORT=3002

# 디버깅 설정
DEBUG=forest:*
LOG_LEVEL=debug 